{
  "name": "zkdj-gulp-cli",
  "version": "1.0.2",
  "description": "一个简单的脚手架，两套常用的gulp自动化模版（PC/Mobile）。",
  "preferGlobal": true,
  "bin": {
    "zkdj": "bin/zkdj"
  },
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "lint": "eslint test/e2e/test*.js lib bin/* --env mocha",
    "e2e": "rimraf test/e2e/mock-template-build && mocha test/e2e/test.js --slow 1000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhecheng-zx/zkdj-gulp-cli.git"
  },
  "keywords": [
    "zkdj",
    "cli",
    "gulp",
    "zhx"
  ],
  "author": "张鑫",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhecheng-zx/zkdj-gulp-cli/issues"
  },
  "homepage": "https://github.com/zhecheng-zx/zkdj-gulp-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "inquirer": "^6.2.0",
    "ora": "^3.0.0",
    "request": "^2.88.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {}
}
